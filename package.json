{
  "name": "cognitiveimagescollection",
  "version": "1.0.0",
  "description": "Analyze images to extract tags and description for images",
  "main": "deploy/serveranalyzer/analyze.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "analyze": "node deploy/outputanalysis/app/src/serveranalyzer/analyze",
    "train": "node deploy/outputanalysis/app/src/serveranalyzer/train",
    "single": "node deploy/outputanalysis/app/src/serveranalyzer/testSingleImageFromTrainingSet",
    "stitch": "node deploy/outputanalysis/app/src/serveranalyzer/stitchMetaInfo",
    "db": "node deploy/outputanalysis/app/src/serveranalyzer/saveInDb",
    "a": "npm run build & npm run analyze",
    "t": "npm run build & npm run train",
    "s": "npm run build & npm run single",
    "x": "npm run build & npm run stitch",
    "sdb": "npm run build & npm run db",
    "clean": "rm -rf ./deploy",
    "copyweb": "copyfiles -u 1 ./app/src/website/index.html ./deploy/outputweb/",
    "packageweb": "./node_modules/.bin/webpack --config webpack.config.js --display-error-details",
    "buildweb": "npm run clean & SET NODE_ENV=development & npm run packageweb & npm run copyweb",
    "webserver": "node bin/www.js"
  },
  "keywords": [
    "analyze",
    "image",
    "cognitive"
  ],
  "author": "Patrick Desjardins",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/glob": "^5.0.30",
    "@types/jquery": "^2.0.46",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^2.2.6",
    "@types/node": "^7.0.31",
    "@types/react": "^15.0.33",
    "@types/react-dom": "^15.5.1",
    "@types/react-hot-loader": "3.0.2",
    "@types/react-redux": "^4.4.45",
    "@types/request": "0.0.44",
    "@types/sharp": "^0.17.2",
    "@types/stream-buffers": "^3.0.2",
    "@types/systemjs": "^0.20.2",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "mongodb": "^2.2.29",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.5",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "form-data": "^2.2.0",
    "glob": "^7.1.2",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "request": "^2.81.0",
    "sharp": "^0.18.1",
    "stream-buffers": "^3.0.1"
  }
}
